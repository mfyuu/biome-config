{
	"compilerOptions": {
		"target": "ESNext",
		"baseUrl": ".",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*", "templates/**/*"],
	"exclude": ["node_modules", "dist"]
}
